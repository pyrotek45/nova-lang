module loopsDemo

// For loop (expression;condition;iteration)
for let i = 0; i < 10; i += 1 {
    println(i)
}

// While loops (condition)
let j = 0
while true {
    j += 1
    if j == 5 {
        continue  // this will jump to the top of the loop
    }
    if j == 10 {
        break  // this will break the loop
    }
    println(j)
}