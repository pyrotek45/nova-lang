module option_typeDemo

// optional types are created with Some(Value) and None(Type)
let option  : Option(Int) = Some(10)
let option2 : Option(Int) = None(Int) // this is a nil value. 

// checking if it exist
if option.isSome() {
    option.unwrap() |> println()
}

if option2.isSome() {
    option2.unwrap() |> println()
}
